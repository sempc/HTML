<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap
	PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"
	"http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="zip">
	
	<select id="retrieveSidoList" resultClass="kr.or.ddit.common.vo.ZipVO">
	SELECT SIDO
	  FROM TB_ZIP
	 GROUP BY SIDO
	 ORDER BY SIDO
	</select>
	
</sqlMap>